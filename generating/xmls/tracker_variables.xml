<tracker_variables>
    <variable name='time_slot_tracker' skip_auto_fill='1'>
        <part name='week'       use_as='str({0})' default="r'.*'" />
        <part name='day'        use_as='str({0})' default="r'.*'" />
        <part name='corpus'     use_as='str({0})' default="r'.*'" />
        <part name='room'       use_as='str({0})' default="r'.*'" />
        <part name='timeslot'   use_as='str({0})' default="r'.*'" />
        <part name='lesson'     use_as='str({0})' default="r'.*'" />
        <part name='group'      use_as="r'\[.*,? ?' + str({0})+ r',? ?.*\]'" default="r'.*'" />
        <part name='type'       use_as='str({0})' default="r'.*'" />
        <part name='teacher'    use_as='str({0})' default="r'.*'" />
    </variable>
    <variable name='corpus_tracker'>
        <part name='corpus'     use_as='str({0})' default="r'.*'" />
        <part name='week'       use_as='str({0})' default="r'.*'" />
        <part name='day'        use_as='str({0})' default="r'.*'" />
        <variants name='column' value='ith'>
            <part name='group'      use_as='str({0})' default="None" />
            <part name='teacher'    use_as='str({0})' default="None" />
            <default value="'_.*'"/>
        </variants>
    </variable>
    <variable name='room_tracker'>
        <part name='room'       use_as='str({0})' default="r'.*'" />
        <part name='corpus'     use_as='str({0})' default="r'.*'" />
        <part name='week'       use_as='str({0})' default="r'.*'" />
        <part name='day'        use_as='str({0})' default="r'.*'" />
        <variants name='column' value='ith'>
            <part name='group'      use_as='str({0})' default="None" />
            <part name='teacher'    use_as='str({0})' default="None" />
            <default value="'_.*'"/>
        </variants>
    </variable>
    <variable name='lesson_id_per_day_tracker'>
        <part name='week'       use_as='str({0})' default="r'.*'" />
        <part name='day'        use_as='str({0})' default="r'.*'" />
        <part name='lesson'     use_as='str({0})' default="r'.*'" />
        <variants name='column' value='ith'>
            <part name='group'      use_as='str({0})' default="None" />
            <part name='teacher'    use_as='str({0})' default="None" />
            <default value="'_.*'"/>
        </variants>
    </variable>
    <variable name='teacher_per_lesson_tracker'>
        <part name='lesson'     use_as='str({0})' default="r'.*'" />
        <part name='teacher'    use_as='str({0})' default="r'.*'" />
    </variable> 
</tracker_variables>