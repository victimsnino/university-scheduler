<decorators>
    <decorator name='_for_corpuses'>
        <for source='self.university.corpuses'>
            <variable name='corpus_i' />
        </for>
    </decorator>
    <decorator name='_for_week_and_day'>
        <for source='self.university.study_weeks_and_days'>
            <variable name='week_i' />
            <variable name='day_i' />
        </for>
    </decorator>    
    <decorator name='_for_week_only'>
        <for source='range(self.university.study_weeks)'>
            <variable name='week_i' />
        </for>
    </decorator>    
    <decorator name='_for_day_only'>
        <for source='self.university.study_days'>
            <variable name='day_i' />
        </for>
    </decorator>    
    <decorator name='_for_timeslots'>
        <for source='range(global_config.time_slots_per_day_available)'>
            <variable name='timeslot_i' />
        </for>
    </decorator>    
    <decorator name='_for_rooms'>
        <for source='self.university.corpuses'>
            <variable name='corpus_i' />
        </for>        
        <for source='self.university.corpuses[corpus_i]'>
            <variable name='room'/>
            <local_variable name='room_i' default='room.room_number'/>
        </for>
    </decorator>
    <decorator name='_for_groups_or_teachers'>
        <for source='self._get_groups_teachers_list()'>
            <variable name='container' />
            <variable name='format_out' />
            <variable name='column' />
        </for>
        <for source='enumerate(container)'>
            <variable name='ith' />
            <variable name='teacher_or_group' />
        </for>
    </decorator> 
    <decorator name='_for_lessons'>
        <for source='enumerate(self.university.lessons)'>
            <variable name='lesson_i' />
            <variable name='lesson' />
        </for>
    </decorator> 
</decorators>